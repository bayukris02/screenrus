<odoo>
     <record id="travel_class_tree" model="ir.ui.view">
        <field name="name">Travel Class_tree</field>
        <field name="model">travel_class</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name" class="fw-bold"/>
            </tree>
        </field>
    </record>


    <record id="travel_class_action" model="ir.actions.act_window">
        <field name="name">Travel Class</field>
        <field name="res_model">travel_class</field>
    </record>


    <menuitem
            id="travel_class_menu"
            name="Travel Class"
            parent="conf_menu_sub"
            action="travel_class_action"
    />

</odoo>